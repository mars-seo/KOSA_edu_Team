<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.media.*?>
<?import javafx.scene.image.*?>
<?import java.lang.*?>
<?import java.util.*?>
<?import javafx.scene.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>


<AnchorPane fx:id="mediaRoot" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="team1.Homvis.player.MediaController">
   <children>
      <StackPane prefHeight="480.0" prefWidth="800.0">
         <children>
            <BorderPane prefHeight="480.0" prefWidth="800.0">
               <bottom>
                  <BorderPane BorderPane.alignment="CENTER">
                     <right>
                        <AnchorPane prefHeight="49.0" prefWidth="117.0" BorderPane.alignment="CENTER">
                           <children>
                              <ImageView fx:id="imgSound" fitHeight="30.0" fitWidth="30.0" layoutX="14.0" layoutY="19.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@playerImg/mute.png" />
                                 </image>
                              </ImageView>
                              <Slider fx:id="volumeSlider" layoutX="49.0" layoutY="27.0" prefHeight="14.0" prefWidth="139.0" />
