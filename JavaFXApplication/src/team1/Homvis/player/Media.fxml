                              </ImageView>
                              <ImageView fx:id="stopBtn" fitHeight="35.0" fitWidth="35.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@playerImg/stop.png" />
                                 </image>
                                 <HBox.margin>
                                    <Insets right="2.0" />
                                 </HBox.margin>
                              </ImageView>
                              <ImageView fx:id="nextBtn" fitHeight="35.0" fitWidth="35.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@playerImg/fast-forward.png" />
                                 </image>
                                 <HBox.margin>
                                    <Insets right="2.0" />
                                 </HBox.margin>
                              </ImageView>
                              <ImageView fx:id="uploadBtn" fitHeight="35.0" fitWidth="35.0" pickOnBounds="true" preserveRatio="true">
                                 <image>
                                    <Image url="@playerImg/eject.png" />
                                 </image>
                              </ImageView>
                           </children>
                           <BorderPane.margin>
                              <Insets left="20.0" />
                           </BorderPane.margin>
                        </HBox>
                     </center>
                  </BorderPane>
               </bottom>
               <right>
                  <AnchorPane>
                     <children>
                        <ListView fx:id="mediaList" layoutY="63.0" maxHeight="-Infinity" prefHeight="370.0" prefWidth="180.0" BorderPane.alignment="BOTTOM_CENTER" />
